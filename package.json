{
  "name": "address-widget",
  "packageManager": "yarn@4.6.0",
  "license": "MIT",
  "author": "Freek van Rijt <freek.vanrijt@myparcel.nl>",
  "type": "module",
  "main": "./dist/main.umd.cjs",
  "module": "./dist/main.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/main.d.ts",
        "default": "./dist/main.js"
      },
      "require": {
        "types": "./dist/main.d.cts",
        "default": "./dist/main.umd.cjs"
      }
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "GENERATE_PREVIEW_HTML=true vite build && vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint --cache --fix .",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "test:update": "vitest run -u",
    "typecheck": "vue-tsc --project tsconfig.base.json --noEmit",
    "api-sdk:generate": "openapi-ts"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.5.7",
    "@myparcel/constants": "^2.2.0",
    "@myparcel/sdk": "^4.1.1",
    "@myparcel/ts-utils": "^1.14.0",
    "@myparcel/vue-form-builder": "^1.0.0-beta.50",
    "@vueuse/core": "^12.4.0",
    "pinia": "^2.3.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@hey-api/openapi-ts": "^0.60.1",
    "@myparcel/prettier-config": "^1.1.0",
    "@myparcel/semantic-release-config": "^5.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.10.6",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.1",
    "semver": "^7.6.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.12",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.2.0"
  },
  "engineStrict": true,
  "volta": {
    "node": "20.18.1",
    "yarn": "4.6.0"
  }
}
